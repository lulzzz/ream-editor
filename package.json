{
  "name": "ream-editor",
  "version": "0.1.0",
  "description": "query databases by writing c# and linq",
  "private": true,
  "main": "electron-main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stofte/ream-editor.git"
  },
  "scripts": {
    "tsc:w": "tsc -w",
    "start": "electron electron-main.js DEBUG",
    "bundle": "node bundle-sfx.js",
    "gulp-build": "gulp build",
    "package_electron_win32": "electron-packager ./%PACKAGE_BASE% --platform=win32 --arch=x64 --overwrite --out=./",
    "package_electron_linux": "electron-packager ./$PACKAGE_BASE --platform=linux --arch=x64 --overwrite --out=./",
    "lint": "tslint -t verbose -e ./app/test/editor-testdata.ts ./app/**/*.ts",
    "postinstall": "typings install & echo {} > typings/tslint.json",
    "generate-for-sqlserver": "node scripts/generate-sql.js sqlserver",
    "generate-for-npgsql": "node scripts/generate-sql.js npgsql",
    "test": "electron-mocha --renderer --require reflect-metadata --require test/mocha-spec-setup.js app/**/*.spec.js",
    "int-test-appveyor": "mocha test/**/*.int.js --appveyor --sqlserver",
    "int-test-travis": "mocha test/**/*.int.js --travis --npgsql",
    "int-test-local-sqlserver": "mocha test/**/*.int.js --local --sqlserver",
    "int-test-local-sqlserver2": "mocha test/**/*.int.js --local --sqlserver2",
    "int-test-local-npgsql": "mocha test/**/*.int.js --local --npgsql"
  },
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/router": "3.0.2",
    "@angular/upgrade": "2.0.2",
    "angular-in-memory-web-api": "0.1.5",
    "angular2-mdl": "2.2.2",
    "codemirror": "5.18.2",
    "core-js": "2.4.1",
    "lodash": "4.15.0",
    "material-design-lite": "1.2.1",
    "moment": "2.15.0",
    "node-uuid": "1.4.7",
    "normalizecss": "3.0.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "winston": "2.2.0",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cssnano": "3.7.4",
    "electron": "1.4.3",
    "electron-mocha": "3.1.0",
    "electron-packager": "8.0.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.0",
    "gulp-cssnano": "2.1.2",
    "gulp-postcss": "6.2.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "6.1.1",
    "gulp-typedoc": "2.0.0",
    "gulp-typescript": "2.13.6",
    "gulp-watch": "4.3.9",
    "lost": "7.1.0",
    "mocha": "3.0.2",
    "postcss-import": "8.1.2",
    "postcss-inline-base64": "2.2.1",
    "postcss-urlrewrite": "0.2.0",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "spectron": "3.3.0",
    "systemjs-builder": "0.15.31",
    "through2": "2.0.1",
    "tslint": "3.15.1",
    "typedoc": "0.4.5",
    "typescript": "2.0.2",
    "typings": "1.3.3",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
