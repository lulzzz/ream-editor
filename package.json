{
  "name": "linq-editor",
  "version": "0.1.0",
  "description": "query databases by writing c# and linq",
  "private": true,
  "main": "electron-main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stofte/linq-editor.git"
  },
  "scripts": {
    "start": "electron electron-main.js DEBUG",
    "ts": "tsc -w",
    "ts-build": "tsc",
    "bundle": "node bundle-sfx.js",
    "gulp-build": "gulp build",
    "package_electron_win32": "electron-packager ./%PACKAGE_BASE% --platform=win32 --arch=x64 --overwrite --out=./",
    "package_electron_linux": "electron-packager ./$PACKAGE_BASE --platform=linux --arch=x64 --overwrite --out=./",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "postinstall": "tsdm rewire",
    "generate-for-sqlserver": "node scripts/generate-sql.js sqlserver",
    "generate-for-npgsql": "node scripts/generate-sql.js npgsql",    
    "test": "mocha app/**/*.spec.js",
    "int-test-sqlserver": "mocha test/**/*.int.js --sqlserver",
    "int-test-npgsql": "mocha test/**/*.int.js --npgsql"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "3.3.6",
    "codemirror": "5.15.2",
    "lodash": "4.12.0",
    "moment": "2.13.0",
    "ng2-bootstrap": "1.0.17",
    "node-uuid": "1.4.7",
    "normalizecss": "3.0.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.29",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cssnano": "^3.7.0",
    "electron-packager": "7.0.3",
    "electron-prebuilt": "0.37.8",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-postcss": "^6.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.6",
    "gulp-watch": "^4.3.6",
    "lost": "^6.8.0",
    "mocha": "2.5.3",
    "postcss-import": "^8.1.2",
    "postcss-inline-base64": "^2.0.0",
    "postcss-urlrewrite": "^0.2.0",
    "spectron": "3.2.3",
    "systemjs-builder": "0.15.19",
    "through2": "^2.0.1",
    "tslint": "3.10.1",
    "typescript": "1.8.10",
    "typings": "^1.0.4",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
